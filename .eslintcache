ect(() => {\n        window.addEventListener(\"scroll\", updownListener);\n        return () => {window.removeEventListener(\"scroll\", updownListener)}\n    }, []);\n\n\n    const switchTech = (index) => {\n        if(scrollingDown) {\n            if(index < imgs.length - 1) {\n                setCurrentIndex(++index);\n            }\n        } else {\n            if(index > 0)\n            setCurrentIndex(--index);\n        }\n    }\n\n\n    return (\n        <section className=\"dev-section\">\n            {!hideScreen && (\n                <div className=\"screen-wrapper\">\n                    <div className=\"screen\">\n                        <div className=\"screen-content\">\n                            <img src={imgs[currentIndex]}/>\n                        </div>\n                    </div>\n                </div>\n            )}\n            <div className=\"tech-blob-wrapper\">\n                {blobs.map((tech, index) => {\n                    return (\n                        <Tech \n                        fullWidth={hideScreen? true : false}\n                        onLeaveViewport={() => switchTech(index)}\n                        title={tech.title}\n                        par={tech.par}></Tech>\n                    )\n                })}\n            </div>\n        </section>\n    )\n}\n\nexport default TechScroll;\n\n","/Users/ridgereventar/Projects/ridgesite/src/pages/Pepi.js",["186","187","188","189","190","191","192","193","194","195","196","197","198","199","200","201","202"],"import React, {useState, useEffect} from 'react';\nimport { Fade } from \"react-reveal\";\n\nimport '../styles/projects/projects.css';\n\nimport Nav from '../components/Nav';\nimport ProjectGrid from '../components/ProjectGrid';\nimport Footer from '../components/Footer';\n\nimport pepiTech from '../images/projects/pepi/pepitech.png';\nimport pepiWet from '../images/projects/pepi/pepiwet.png';\nimport pepiSpace from '../images/projects/pepi/pepispace.png';\n\nimport 