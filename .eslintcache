d) {\n            ticking = false;\n            return;\n          }\n          setScrollDir(scrollY > lastScrollY ? \"down\" : \"up\");\n          lastScrollY = scrollY > 0 ? scrollY : 0;\n          ticking = false;\n        };\n      \n        const onScroll = () => {\n          if (!ticking) {\n            window.requestAnimationFrame(updateScrollDir);\n            ticking = true;\n          }\n        };\n      \n        window.addEventListener(\"scroll\", onScroll);\n        console.log(scrollDir);\n      \n        return () => window.removeEventListener(\"scroll\", onScroll);\n      }, [scrollDir]);\n\n    const setAllFalse = () => {\n        setShow1(false);\n        setShow2(false);\n        setShow3(false);\n        setShow4(false);\n        setShow5(false);\n    }\n\n    const switchTech = (index) => {\n        setAllFalse();\n        console.log(index);\n        \n        if(scrollDir === \"down\") {\n            if(index < 4) {\n                setArray[index + 1](true);\n            } else if (index == 4) {\n                setArray[index](true);\n            }    \n        } else {\n            if(index > 0) {\n                setArray[index - 1](true);\n            } else if(index == 0) {\n                setArray[index](true);\n            }\n        }\n\n    }\n\n    return (\n        <React.Fragment>\n            \n            <Nav proj={true}/>\n            \n            <div className=\"proj-container proj-hero\">\n                <Fade left distance=\"80px\">\n                    <img src={suiLogo}></img>\n                    <p>A Style Guide & Ui Component Generator</p>\n                    <Trigger onLeaveViewport={() => setPlayAnim(true)}></Trigger>        \n                </Fade>\n            </div>\n\n            <div className=\"proj-container breakdown\">\n                <Fade>\n                    <div className=\"roles-container\">\n                        <div>\n                            <h2>Deliverables</h2>\n                     